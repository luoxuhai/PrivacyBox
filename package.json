    "specs": "detox",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/PrivateSpace.app",
        "build": "xcodebuild -workspace ios/PrivateSpace.xcworkspace -scheme PrivateSpace -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 14'",
        "type": "ios.simulator",
        "device": {
          "name": "iPhone 14",
          "os": "iOS 15.5"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/PrivateSpace.app",
        "build": "xcodebuild -workspace ios/PrivateSpace.xcworkspace -scheme PrivateSpace -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 14'",
        "type": "ios.simulator",
        "device": {
          "name": "iPhone 14",
          "os": "iOS 15.5"
        }
      },
      "ios.sim.expo": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 14"
      }
    }
